{
  "name": "jarvis-agent",
  "version": "1.0.0",
  "description": "JARVIS - Next-generation autonomous AI operative with local-first architecture",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "setup": "tsx scripts/setup-wizard.ts",
    "quick-start": "tsx scripts/quick-start.ts",
    "cli": "tsx scripts/quick-start.ts --cli",
    "test": "vitest",
    "test:unit": "vitest run tests/unit",
    "test:integration": "vitest run tests/integration",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src/**/*.ts",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "ai",
    "agent",
    "llm",
    "claude",
    "autonomous",
    "assistant"
  ],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=22.0.0"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.39.0",
    "@google/generative-ai": "^0.21.0",
    "@types/ws": "^8.18.1",
    "dotenv": "^16.4.7",
    "ollama": "^0.5.12",
    "openai": "^4.77.0",
    "winston": "^3.17.0",
    "ws": "^8.19.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/node": "^22.19.7",
    "@typescript-eslint/eslint-plugin": "^8.19.1",
    "@typescript-eslint/parser": "^8.19.1",
    "@vitest/coverage-v8": "^2.1.8",
    "eslint": "^9.17.0",
    "playwright": "^1.58.1",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  }
}